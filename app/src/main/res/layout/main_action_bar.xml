<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tableInvoices"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="0dp"
    android:stretchColumns="*"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="UselessParent">
        <TextView
            style="@style/toolbarIconStyle"
            android:layout_column="1"
            android:drawablePadding="5dp"
            android:paddingStart="1dp"
            android:paddingEnd="10dp"
            android:gravity="center"
            android:layout_gravity="center"
            android:text="-"
            android:id="@+id/main_refer_money"
            android:textSize="25sp"
            app:drawableLeftCompat="@drawable/refer_money" />
        <TextView
            android:text="-"
            android:textSize="25sp"
            style="@style/toolbarIconStyle"
            android:paddingStart="1dp"
            android:paddingEnd="10dp"
            android:gravity="center"
            android:layout_gravity="center"
            android:id="@+id/main_win_money"
            android:drawablePadding="5dp"
            android:layout_column="2"
            app:drawableLeftCompat="@drawable/win_money" />
        <TextView
            android:text="-"
            android:textSize="25sp"
            android:paddingStart="1dp"
            android:paddingEnd="10dp"
            android:drawablePadding="5dp"
            android:gravity="center"
            android:layout_gravity="center"
            style="@style/toolbarIconStyle"
            android:id="@+id/main_main_money"
            android:layout_column="3"
            app:drawableLeftCompat="@drawable/wallet2_money" />
        <ImageView
            android:src="@drawable/upcoming"
            android:id="@+id/celebritySchedule"
            style="@style/toolbarIconStyle"
            android:layout_column="4"
            android:visibility="gone"
            android:contentDescription="@string/chat_msg" />
        <ImageView
            android:src="@drawable/search"
            android:id="@+id/search"
            style="@style/toolbarIconStyle"
            android:layout_column="5"
            android:visibility="gone"
            android:contentDescription="@string/chat_msg" />
        <ImageView
            android:src="@drawable/test_chat_bkp"
            style="@style/toolbarIconStyle"
            android:id="@+id/chat"
            android:layout_column="6"
            android:contentDescription="@string/chat_msg" />
    </TableRow>
</TableLayout>