<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:layout_alignParentStart="true"
        android:layout_centerInParent="true"
        android:src="@drawable/coins"
        android:id="@+id/moneyIcon"
        android:paddingStart="@dimen/action_bar_icon_start_padding"
        android:paddingEnd="@dimen/action_bar_icon_end_padding"
        android:gravity="start"
        android:layout_gravity="start"
        android:layout_width="34dp"
        android:layout_height="wrap_content"
        android:contentDescription="@string/chat_msg" />
    <TextView
        android:layout_toEndOf="@+id/moneyIcon"
        android:layout_centerInParent="true"
        style="@style/toolbarIconStyle"
        android:text="@string/toolbar_ob_default"
        android:textSize="@dimen/action_bar_text_size"
        android:paddingStart="@dimen/action_bar_icon_start_padding"
        android:paddingEnd="@dimen/action_bar_icon_end_padding"
        android:gravity="start"
        android:layout_gravity="start"
        android:id="@+id/main_main_money"/>
    <ImageView
        android:layout_alignParentEnd="true"
        android:id="@+id/settings"
        android:src="@drawable/settingshas"
        android:layout_marginStart="@dimen/margin_top_medium"
        android:layout_marginEnd="@dimen/margin_top_medium"
        android:paddingStart="@dimen/action_bar_icon_start_padding"
        android:paddingEnd="@dimen/action_bar_icon_end_padding"
        android:enabled="false"
        style="@style/toolbarIconStyle"
        android:contentDescription="@string/chat_msg" />
    <ImageView
        android:src="@drawable/help"
        android:id="@+id/help"
        android:layout_toStartOf="@+id/settings"
        android:layout_marginStart="@dimen/margin_top_medium"
        android:layout_marginEnd="@dimen/margin_top_medium"
        android:paddingStart="@dimen/action_bar_icon_start_padding"
        android:paddingEnd="@dimen/action_bar_icon_end_padding"
        android:enabled="false"
        style="@style/toolbarIconStyle"
        android:contentDescription="@string/chat_msg" />
    <ImageView
        android:layout_toStartOf="@+id/help"
        android:src="@drawable/clockhasini"
        android:id="@+id/celebritySchedule"
        android:layout_marginStart="@dimen/margin_top_medium"
        android:layout_marginEnd="@dimen/margin_top_medium"
        android:paddingStart="@dimen/action_bar_icon_start_padding"
        android:paddingEnd="@dimen/action_bar_icon_end_padding"
        style="@style/toolbarIconStyle"
        android:enabled="false"
        android:contentDescription="@string/chat_msg" />
    <ImageView
        android:layout_toStartOf="@+id/celebritySchedule"
        android:src="@drawable/messagehas"
        android:id="@+id/chat"
        android:enabled="false"
        android:layout_marginStart="@dimen/margin_top_medium"
        android:layout_marginEnd="@dimen/margin_top_medium"
        android:paddingStart="@dimen/action_bar_icon_start_padding"
        android:paddingEnd="@dimen/action_bar_icon_end_padding"
        style="@style/toolbarIconStyle"
        android:contentDescription="@string/chat_msg" />
    <ImageView
        android:layout_toStartOf="@+id/chat"
        android:src="@drawable/searchhas"
        android:id="@+id/search"
        android:enabled="false"
        android:layout_marginStart="@dimen/margin_top_medium"
        android:layout_marginEnd="@dimen/margin_top_medium"
        android:paddingStart="@dimen/action_bar_icon_start_padding"
        android:paddingEnd="@dimen/action_bar_icon_end_padding"
        style="@style/toolbarIconStyle"
        android:contentDescription="@string/chat_msg" />

</RelativeLayout>
